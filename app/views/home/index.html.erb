<title>Clubpulsarracing</title>
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.4.1/css/all.css" integrity="sha384-5sAR7xN1Nv6T6+dT2mhtzEpVJvfS3NScPQTrOxhwjIuvcA67KV2R5Jz6kr4abQsz" crossorigin="anonymous">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <div class="home-wrapper">
      <header>
        <div class="container">
            <%= image_tag("logo.png")%> Club Pulsar Racing Medellin 
          <nav>
            <a href="#home" class="active">HOME</a>
            <a href="#imagenes">IMAGENES</a>
            <a href="#videos">VIDEOS</a>
            <a href="#contacto">CONTACTO</a>
          </nav>
        </div>
      </header>
      
      <section id="home" class="container">
        <h1>VENCE TUS <strong>MIEDOS</strong> Y SAL A <strong>RECORRER</strong> TU CAMÍNO</h1>
        <div class="divider"></div>
        <p>Club Pulsar Racing Medellin, no solo un club, una Familia</p>
        <div class="social">
          <a href=""><i class="fab fa-facebook"></i></a>
          <a href=""><i class="fab fa-twitter"></i></a>
          <a href=""><i class="fab fa-github"></i></a>
        </div>
      </section>
    </div>
    
<section id="imagenes">
  <div class="container">
    
    <h2>IMAGENES</h2>
    <div class = "box">
      <%= image_tag("foto2.jpg")%>
      <h3>CURSO DE MECANICA</h3>
    </div>
    <div class = "box">

      <%= image_tag("foto5.jpg")%>
      <h3>ANIVERSARIO</h3>
    </div>
    <div class = "box">
      <%= image_tag("foto8.jpg")%>
      <h3>REUNIONES</h3>
    </div>
    <div class = "box">
      <%= image_tag("imagen1.jpg")%>
      <h3>Correria a Jardin</h3>
    </div>
    <div class = "box">
      <%= image_tag("foto7.jpg")%>
      <h3>Correria a Jericó</h3>
    </div>
  </div>
</section>


<section id="videos">
   <div class="container">
    <h2>VIDEOS</h2>
     
         <div class="box">
           <iframe width="300" height="200" src="https://www.youtube.com/embed/BgQqQNcBZjQ" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
           <h3>Valle del Cocora</h3>
         </div>
         <div class="box">
            <iframe width="300" height="200" src="https://www.facebook.com/ClubPulsarRacingMedellin/videos/1743614559020762/" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
           <h3>JERICÓ</h3>
        </div>
         <div class="box">
           <iframe width="300" height="200" src="https://www.facebook.com/ClubPulsarRacingMedellin/videos/186645265387672/" frameborder="0" allow="autoplay; encrypted-media" allowfullscreen></iframe>
           <h3>CISNEROS</h3>
         </div>
   </div>
</section>


<section id="contacto">
  <h2>CONTACTO</h2>
      <form>
          <div><input type="text" placeholder="Tu Email"></div>
            <div><textarea rows = "10" placeholder="Tu Mensaje"></textarea></div>
            <button>ENVIAR</button>
      </form>
</section>

